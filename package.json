{
  "name": "cv-post-graduate",
  "version": "1.0.0",
  "description": "Curriculum Vitae for Post-Graduate Application",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "chokidar-cli": "^2.1.0",
    "http-server": "^0.12.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "sass": "^1.25.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-task:scss-compile": "node-sass-chokidar --source-map true scss/ -o css",
    "build-task:autoprefixer": "postcss css/*.css --use autoprefixer -d dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/**/*.scss' -c 'npm run sass:build'",
    "start": "npm run dev",
    "serve": "http-server",
    "dev": "npm-run-all -p sass:* serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PedroLimaSilva/cv-post-graduate.git"
  },
  "keywords": [
    "resume",
    "application",
    "cv"
  ],
  "author": "Pedro Lima e Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PedroLimaSilva/cv-post-graduate/issues"
  },
  "homepage": "https://github.com/PedroLimaSilva/cv-post-graduate#readme"
}
